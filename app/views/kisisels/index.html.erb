<%- model_class = Kisisel -%>
<div class="page-header">
  <h1><%=t '.title', default: model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:basvuran_id) %></th>
      <th><%= model_class.human_attribute_name(:dogum_yili) %></th>
      <th><%= model_class.human_attribute_name(:meslek) %></th>
      <th><%= model_class.human_attribute_name(:is_durumu) %></th>
      <th><%= model_class.human_attribute_name(:sosyal_guvence_durumu) %></th>
      <th><%= model_class.human_attribute_name(:sosyal_guvence_durumu_ssk_free) %></th>
      <th><%= model_class.human_attribute_name(:sosyal_guvence_durumu_diger_free) %></th>
      <th><%= model_class.human_attribute_name(:il_ilce) %></th>
      <th><%= model_class.human_attribute_name(:medeni_durum) %></th>
      <th><%= model_class.human_attribute_name(:evlilik_yili) %></th>
      <th><%= model_class.human_attribute_name(:cocuk_sayisi) %></th>
      <th><%= model_class.human_attribute_name(:uyruk) %></th>
      <th><%= model_class.human_attribute_name(:uyruk_diger_free) %></th>
      <th><%= model_class.human_attribute_name(:dili) %></th>
      <th><%= model_class.human_attribute_name(:cevirmen_ihtiyaci) %></th>
      <th><%= model_class.human_attribute_name(:cevirmen_ihtiyaci_evet_free) %></th>
      <th><%= model_class.human_attribute_name(:engellilik_durumu) %></th>
      <th><%= model_class.human_attribute_name(:engellilik_durumu_var_free) %></th>
      <th><%= model_class.human_attribute_name(:hamilelik) %></th>
      <th><%= model_class.human_attribute_name(:ruhsal_destek) %></th>
      <th><%= model_class.human_attribute_name(:ruhsal_destek_evet_free) %></th>
      <th><%= model_class.human_attribute_name(:intihar_girisimi) %></th>
      <th><%= model_class.human_attribute_name(:notlar) %></th>
      <th><%= model_class.human_attribute_name(:hamilelik_evet_tarih) %></th>
      <th><%= model_class.human_attribute_name(:hamilelik_evet_doktor) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', default: t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @kisisels.each do |kisisel| %>
      <tr>
        <td><%= kisisel.id %></td>
          <td><%= kisisel.basvuran_id %></td>
          <td><%= kisisel.dogum_yili %></td>
          <td><%= kisisel.meslek %></td>
          <td><%= kisisel.is_durumu %></td>
          <td><%= kisisel.sosyal_guvence_durumu %></td>
          <td><%= kisisel.sosyal_guvence_durumu_ssk_free %></td>
          <td><%= kisisel.sosyal_guvence_durumu_diger_free %></td>
          <td><%= kisisel.il_ilce %></td>
          <td><%= kisisel.medeni_durum %></td>
          <td><%= kisisel.evlilik_yili %></td>
          <td><%= kisisel.cocuk_sayisi %></td>
          <td><%= kisisel.uyruk %></td>
          <td><%= kisisel.uyruk_diger_free %></td>
          <td><%= kisisel.dili %></td>
          <td><%= kisisel.cevirmen_ihtiyaci %></td>
          <td><%= kisisel.cevirmen_ihtiyaci_evet_free %></td>
          <td><%= kisisel.engellilik_durumu %></td>
          <td><%= kisisel.engellilik_durumu_var_free %></td>
          <td><%= kisisel.hamilelik %></td>
          <td><%= kisisel.ruhsal_destek %></td>
          <td><%= kisisel.ruhsal_destek_evet_free %></td>
          <td><%= kisisel.intihar_girisimi %></td>
          <td><%= kisisel.notlar %></td>
          <td><%= kisisel.hamilelik_evet_tarih %></td>
          <td><%= kisisel.hamilelik_evet_doktor %></td>
        <td><%=l kisisel.created_at, format: :long %></td>
        <td>
          <%= link_to kisisel_path(kisisel), class: 'btn btn-xs', title: "#{ t('.show', default: t('helpers.links.show')) }" do %>
            <%= glyph 'info-sign' %>
          <%- end -%>
          <%= link_to edit_kisisel_path(kisisel), class: 'btn btn-xs', title: "#{ t('.edit', default: t('helpers.links.edit')) }" do %>
            <%= glyph 'pencil' %>
          <%- end -%>
          <%= link_to kisisel_path(kisisel), method: :delete, data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Are you sure?')) }, class: 'btn btn-xs', title: "#{ t('.destroy', default: t('helpers.links.destroy')) }" do %>
            <%= glyph 'remove' %>
          <%- end -%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', default: t("helpers.links.new")),
            new_kisisel_path,
            class: 'btn btn-primary' %>
